NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
NOT(
OR(
 AND( 
  AgreementId1__c = &apos;agreementForTermsAndConditions&apos;,
  AgreementId2__c = &apos;agreementForMarketingCommunicationByPhone&apos;,
  AgreementId3__c = &apos;agreementForMarketingCommunicationByEmail&apos;
 ),
 AND( 
  AgreementId1__c = &apos;agreementForTermsAndConditions&apos;,
  AgreementId3__c = &apos;agreementForMarketingCommunicationByPhone&apos;,
  AgreementId2__c = &apos;agreementForMarketingCommunicationByEmail&apos;
 ),
 AND( 
  AgreementId2__c = &apos;agreementForTermsAndConditions&apos;,
  AgreementId1__c = &apos;agreementForMarketingCommunicationByPhone&apos;,
  AgreementId3__c = &apos;agreementForMarketingCommunicationByEmail&apos;
 ),
 AND( 
  AgreementId2__c = &apos;agreementForTermsAndConditions&apos;,
  AgreementId3__c = &apos;agreementForMarketingCommunicationByPhone&apos;,
  AgreementId1__c = &apos;agreementForMarketingCommunicationByEmail&apos;
 ),
 AND( 
  AgreementId3__c = &apos;agreementForTermsAndConditions&apos;,
  AgreementId1__c = &apos;agreementForMarketingCommunicationByPhone&apos;,
  AgreementId2__c = &apos;agreementForMarketingCommunicationByEmail&apos;
 ),
 AND( 
  AgreementId3__c = &apos;agreementForTermsAndConditions&apos;,
  AgreementId2__c = &apos;agreementForMarketingCommunicationByPhone&apos;,
  AgreementId1__c = &apos;agreementForMarketingCommunicationByEmail&apos;
 )
) 
)
)</errorConditionFormula>
        <errorMessage>Wrong agreemntes id. Accepted are agreementForTermsAndConditions, agreementForMarketingCommunicationByPhone, agreementForMarketingCommunicationByEmail</errorMessage>
