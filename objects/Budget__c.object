        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), Share_of_Wallet_MUSD__c &gt;= Total_Spend_MUSD_1__c)</errorConditionFormula>
        <errorMessage>“ABB Orders MUSD” must be less than the sum of “Total Account CAPEX MUSD” and &quot;Total Account OPEX MUSD&quot;&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_plan_year_comparison</fullName>
        <active>true</active>
        <description>R5</description>
        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), IF (TEXT(Account_Plan_Year__c) != NULL &amp;&amp; TEXT(Account_Plan__r.Account_Plan_Year__c) != NULL, VALUE(TEXT(Account_Plan_Year__c)) &lt; (VALUE(TEXT(Account_Plan__r.Account_Plan_Year__c))- 3), FALSE))</errorConditionFormula>
        <errorMessage>Sales Targets &amp; Results Year must be either greater than Account Plan Year or past three years from Account Plan Year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Available_CAPEX_less_than_Total_CAPEX</fullName>
        <active>true</active>
        <description>R5 - Available Market CAPEX MUSD must be less than Total Market CAPEX MUSD.</description>
        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), Available_Market_CAPEX_MUSD__c &gt; Total_Market_CAPEX_MUSD__c)</errorConditionFormula>
        <errorDisplayField>Available_Market_CAPEX_MUSD__c</errorDisplayField>
        <errorMessage>Available Account CAPEX MUSD must be less than Total Account CAPEX MUSD!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Available_OPEX_less_than_Total_OPEX</fullName>
        <active>true</active>
        <description>R5 - Available Market OPEX MUSD must be less than Total Market OPEX MUSD.</description>
        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), Available_Market_OPEX_MUSD__c &gt; Total_Market_OPEX_MUSD__c)</errorConditionFormula>
        <errorDisplayField>Available_Market_OPEX_MUSD__c</errorDisplayField>
        <errorMessage>Available Account OPEX MUSD must be less than Total Account OPEX MUSD!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_edit_Actual_Sales_if_Sync_is_set</fullName>
        <active>true</active>
        <description>Created as part of R8 - CR (C191316)</description>
        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), NOT($Profile.Name = $Label.Sys_Admin_Profile_Name) &amp;&amp; Automatic_Sync__c &amp;&amp; NOT(Automatic_Sync_Technical__c) &amp;&amp; ISCHANGED( Actual_Sales_USD__c ))</errorConditionFormula>
        <errorDisplayField>Actual_Sales_USD__c</errorDisplayField>
        <errorMessage>Manual update of the field is not allowed when &apos;Automatic Sync&apos; is set as true.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_editable_only_by_record_creator</fullName>
        <active>true</active>
        <description>Comments__c editable only by record (sales target and results) creator</description>
        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), ISCHANGED(Comments__c),($User.Id  &lt;&gt;  CreatedById))</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Comments can be edited only by the record creator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Financial_Target_Status_Closed_Check</fullName>
        <active>true</active>
        <description>R5 - If Account Plan Status is Closed, then the user should not edit the Financial Target and Expectation.</description>
        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), ISPICKVAL(Account_Plan__r.Status__c, &apos;Closed&apos;))</errorConditionFormula>
        <errorMessage>You cannot save the changes - Account Plan is Closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Segment_Level_Validation</fullName>
        <active>true</active>
        <description>R5: Segment level must always match the hierarchy entered by the user in Division/Business Unit/Product Group/Product Line</description>
        <errorConditionFormula>AND( 
NOT($Setup.Validation_Override__c.Exclude__c), 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), 
OR(
            (TEXT(Business_Unit__c) != NULL &amp;&amp; TEXT(Segment_Level__c) == &apos;Division&apos;),
            (TEXT(Business_Unit__c) == NULL &amp;&amp; TEXT(Segment_Level__c) == &apos;Business Unit&apos;),
            ( (Product_Group__c != NULL || Product__c != NULL) &amp;&amp; (TEXT(Segment_Level__c) == &apos;Division&apos; || TEXT(Segment_Level__c) == &apos;Business Unit&apos;)),
            ( (Product__c == NULL) &amp;&amp; TEXT(Segment_Level__c) == &apos;Product Line&apos;),
            ( (Product_Group__c == NULL) &amp;&amp; TEXT(Segment_Level__c) == &apos;Product Group&apos;),
            ( (Product_Group__c != NULL &amp;&amp; Product__c != NULL) &amp;&amp; TEXT(Segment_Level__c) != &apos;Product Line&apos;)))</errorConditionFormula>
        <errorMessage>Segment level must always match the hierarchy entered by the user in Division/Business Unit/Product Group/Product Line</errorMessage>
    </validationRules>
