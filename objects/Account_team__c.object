ISPICKVAL( Team_Role__c ,&apos;Ext Agent&apos;)  ||  ISPICKVAL(Team_Role__c,&apos;Ext Channel Partner&apos;)  ||  ISPICKVAL(Team_Role__c,&apos;Ext Rep&apos;)
