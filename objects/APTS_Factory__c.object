        <errorConditionFormula>AND(APTS_Gap_Factor__c &gt; 6,
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
NOT(ISBLANK(APTS_Gap_Factor__c)))</errorConditionFormula>
        <errorMessage>Gap factor should not be more than 6 months.</errorMessage>
    </validationRules>
