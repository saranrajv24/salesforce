        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/apex.js&quot;)}

var costtypes = sforce.connection.query(&quot;SELECT Id from Apttus_Config2__CostType__c WHERE Apttus_Config2__CostModelId__c  = &apos;{!APTS_Factory__c.Apttus_Config2_CostModelIdId__c}&apos; LIMIT 1&quot;);
var records = costtypes.getArray(&quot;records&quot;);
if(records.length&gt;0) {
	alert(&quot;Cost Model is already associated with this factory&quot;);
} else {

	sforce.apex.execute(&quot;APTS_GenerateFactoryCostRecord&quot;,&quot;generateCostRecordUsingBatch&quot;,{factoryId:&quot;{!APTS_Factory__c.Id}&quot;});
	alert(&quot;Record For Cost Model will be updated in sometime.&quot;);
	location.reload(true);
}</url>
