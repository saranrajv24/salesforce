    <validationRules>
        <fullName>Date_Values_cannot_be_Past_Date</fullName>
        <active>true</active>
        <description>R4 Check the Date value cannot be entered as a past date</description>
        <errorConditionFormula>AND(
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), 
OR(AND(NOT(ISNULL(Customer_Pilot_Installation_Start_Date__c)), Customer_Pilot_Installation_Start_Date__c &lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(End_Date__c)), End_Date__c &lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setStep1NQual__c)), Date_when_Step_is_setStep1NQual__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setStep2NQual__c)), Date_when_Step_is_setStep2NQual__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setExecution1__c)), Date_when_Step_is_setExecution1__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setExecution2__c)), Date_when_Step_is_setExecution2__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setExecution3__c)), Date_when_Step_is_setExecution3__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setExecution4__c)), Date_when_Step_is_setExecution4__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setExecution5__c)), Date_when_Step_is_setExecution5__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setPlanning1__c)), Date_when_Step_is_setPlanning1__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setPlanning2__c)), Date_when_Step_is_setPlanning2__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setPlanning3__c)), Date_when_Step_is_setPlanning3__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setPlanning4__c)), Date_when_Step_is_setPlanning4__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setQualified1__c)), Date_when_Step_is_setQualified1__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setQualified2__c)), Date_when_Step_is_setQualified2__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setSubmitted1__c)), Date_when_Step_is_setSubmitted1__c&lt; Product_Qualification__r.Startdate__c),
AND(NOT(ISNULL(Date_when_Step_is_setSubmitted2__c)), Date_when_Step_is_setSubmitted2__c&lt; Product_Qualification__r.Startdate__c)))</errorConditionFormula>
        <errorMessage>Date values cannot be before the Product Qualification Start Date</errorMessage>
