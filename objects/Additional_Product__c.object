    if(
       AND(
           NOT(ISNULL(Customer_Warranty_startdate__c)),
           NOT(ISNULL(Customer_Warranty_enddate__c)),
           Customer_Warranty_startdate__c &lt;= TODAY(),
           TODAY() &lt;= Customer_Warranty_enddate__c
          ),
      &apos;Active&apos;,
      if(
