    <fields>
        <fullName>Heat_Index__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Technical_Calculation_3__c = 100/100, IMAGE(&quot;/resource/Order_Booked&quot;, &quot;Orders Booked&quot;, 20, 30),
IF( AND(Technical_Calculation_3__c &gt;= 50/100, Technical_Calculation_3__c &lt;= 99/100), IMAGE(&quot;/resource/Hot&quot;, &quot;Hot&quot;, 20, 20),
IF( AND(Technical_Calculation_3__c &gt;= 12/100, Technical_Calculation_3__c &lt;= 49/100), IMAGE(&quot;/resource/Warm&quot;, &quot;Warm&quot;, 20, 20),
IF( AND(Technical_Calculation_3__c &gt;= 0/100, Technical_Calculation_3__c &lt;= 11/100), IMAGE(&quot;/resource/Cold&quot;, &quot;Cold&quot;, 20, 20), Null
))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Heat Index</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
