        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Customer_buying_path__c,&apos;Awareness&apos;),(0/100),
IF(ISPICKVAL(Customer_buying_path__c,&apos;Discovery&apos;),(10/100),
IF(ISPICKVAL(Customer_buying_path__c,&apos;Explore&apos;),(40/100),
IF(ISPICKVAL(Customer_buying_path__c,&apos;Tendering&apos;),(70/100),
