    <validationRules>
        <fullName>JBP_Status_Closed_Check_SalesTargets</fullName>
        <active>true</active>
        <description>R5 - If Channel Partner Business Plan Status is Closed/Obsolete, then the user should not edit the Sales Targets and Results</description>
        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), OR(ISPICKVAL( Joint_Business_Plan__r.Status__c , &apos;Closed&apos;), 
ISPICKVAL( Joint_Business_Plan__r.Status__c , &apos;Obsolete&apos;) 
))</errorConditionFormula>
        <errorMessage>You cannot save the changes - Channel Partner Business Plan is Closed/Obsolete</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_the_fields_to_Edit_When_Active</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
ISPICKVAL(Joint_Business_Plan__r.Status__c, &apos;Active&apos;),      (ISCHANGED(Current_Year_Currency__c) ||
 ISCHANGED(Starting_Year__c) || 
 ISCHANGED(Second_Year_Currency__c) || 
 ISCHANGED(Third_Year_Currency__c) || 
 ISCHANGED(Previous_Year_Currency__c) ))</errorConditionFormula>
        <errorMessage>You are not allowed to change the values of Current Year,Budget Current Year, Target Second Year, Target Third Year and Sales Previous Year when the channel Business Plan Status is Active</errorMessage>
    </validationRules>
