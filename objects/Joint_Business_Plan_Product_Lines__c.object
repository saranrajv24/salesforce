        <fullName>Run_Rate_On__c</fullName>
        <description>R5 - Run Rate Calculation Formula</description>
        <externalId>false</externalId>
        <formula>IF (ISNULL(Actual_Year_to_Date_Changed_on__c) , 0, (365  / (  Actual_Year_to_Date_Changed_on__c - DATEVALUE( TEXT(YEAR( Actual_Year_to_Date_Changed_on__c  )) &amp; &quot;-01-01&quot; )))  * Current_Ongoing_Year__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Calculated field; linear projection of Full Year run rate based YTD Sales and Date</inlineHelpText>
        <label>Run Rate Current Year</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
