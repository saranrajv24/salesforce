    <validationRules>
        <fullName>ABB_Orders_MUSD_less_than_the_TotalSpend</fullName>
        <active>true</active>
        <errorConditionFormula>Share_of_Wallet_MUSD__c &gt;=  Total_Spend_MUSD_1__c</errorConditionFormula>
        <errorMessage>“ABB Orders MUSD” must be less than the sum of “Total Account CAPEX MUSD” and &quot;Total Account OPEX MUSD”&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_plan_year_comparison</fullName>
        <active>true</active>
        <description>R5</description>
        <errorConditionFormula>IF ( TEXT(Account_Plan_Year__c) != NULL &amp;&amp; TEXT(Account_Plan__r.Account_Plan_Year__c) != NULL, VALUE(TEXT(Account_Plan_Year__c)) &lt; (VALUE(TEXT(Account_Plan__r.Account_Plan_Year__c))-3), FALSE)</errorConditionFormula>
        <errorMessage>Year must be either greater than Account Plan Year or past three years from Account Plan Year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Available_CAPEX_less_than_Total_CAPEX</fullName>
        <active>true</active>
        <description>R5 - Available Market CAPEX MUSD must be less than Total Market CAPEX MUSD.</description>
        <errorConditionFormula>Available_Market_CAPEX_MUSD__c  &gt;  Total_Market_CAPEX_MUSD__c</errorConditionFormula>
        <errorDisplayField>Available_Market_CAPEX_MUSD__c</errorDisplayField>
        <errorMessage>Available Account CAPEX MUSD must be less than Total Account CAPEX MUSD!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Available_OPEX_less_than_Total_OPEX</fullName>
        <active>true</active>
        <description>R5 - Available Market OPEX MUSD must be less than Total Market OPEX MUSD.</description>
        <errorConditionFormula>Available_Market_OPEX_MUSD__c  &gt;  Total_Market_OPEX_MUSD__c</errorConditionFormula>
        <errorDisplayField>Available_Market_OPEX_MUSD__c</errorDisplayField>
        <errorMessage>Available Account OPEX MUSD must be less than Total Account OPEX MUSD!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Market_Expectation_Status_Closed_Check</fullName>
        <active>true</active>
        <description>R5 - If Account Plan Status is Closed, then the user should not edit the Market Expectation.</description>
        <errorConditionFormula>ISPICKVAL(Account_Plan__r.Status__c, &apos;Closed&apos;)</errorConditionFormula>
        <errorMessage>You cannot save the changes - Account Plan is Closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_less_than_End_Date</fullName>
        <active>true</active>
        <description>R5 - Start Date must be less than End Date.</description>
        <errorConditionFormula>Start_Date__c  &gt;  End_Date__c</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>Start Date must be less than End Date!</errorMessage>
    </validationRules>
