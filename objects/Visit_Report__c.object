    <validationRules>
        <fullName>ChannelBusReview_ClosedCheck_VisitReport</fullName>
        <active>true</active>
        <description>R5 - If Channel Business review Status is Obsolete, then the user should not edit the visit report record</description>
        <errorConditionFormula>AND(NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
OR(ISPICKVAL(  Channel_Review__r.Status__c  , &apos;Obsolete&apos;), 
ISPICKVAL( Channel_Review__r.Status__c , &apos;Completed&apos;)))</errorConditionFormula>
        <errorMessage>You cannot save the changes - Channel Partner Business Review is Closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ChannelPreQual_VisitReport_CloseCheck</fullName>
        <active>true</active>
        <description>R5 - If Channel Partner Pre Qualification Status is Obsolete/Declined/Completed, then the user should not edit the Visit report record</description>
        <errorConditionFormula>AND(NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
OR( ISPICKVAL( Channel_Pre_Qualification__r.Status__c , &apos;Obsolete&apos;), ISPICKVAL(Channel_Pre_Qualification__r.Status__c, &apos;Declined&apos;),
ISPICKVAL(Channel_Pre_Qualification__r.Status__c, &apos;Completed&apos;) ))</errorConditionFormula>
        <errorMessage>You cannot save the changes - Channel Partner Pre Qualification is Closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Visit_On_Inactive_Bulk_Accounts</fullName>
        <active>true</active>
        <description>No Visit Reports On Inactive, Bulk Business Accounts</description>
        <errorConditionFormula>OR 
(
    (IF	
	   (AND(NOT($User.AccountMerge_Override__c), 
	    Account__r.RecordType.Name = &quot;Read Only&quot;,
	    NOT($Setup.Validation_Override__c.Exclude__c),
	    ISNEW()), True, False)
    ),
    (IF (CONTAINS( Account__r.Name , &quot;Bulk Business&quot;), True,False))   
)</errorConditionFormula>
        <errorMessage>You can not create a new Visit Report on an Account that is set to Inactive or is a Bulk Business.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_the_changes_in_Year</fullName>
        <active>true</active>
        <description>R6</description>
        <errorConditionFormula>AND(NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),($User.Id != OwnerId &amp;&amp; $Profile.Name != &apos;System Administrator&apos; &amp;&amp; $Profile.Name != &apos;Global UAM Admin&apos; &amp;&amp; $Profile.Name != &apos;ABB Basic Admin&apos;),
(YEAR( DATEVALUE(Start_Date__c) ) !=  PRIORVALUE(Start_Date_Year__c ) || 
YEAR( DATEVALUE(End_Date__c) ) !=  PRIORVALUE(End_Date_Year__c)))</errorConditionFormula>
        <errorMessage>You can&apos;t change the Year of Start Date Time / End Date Time after creation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDate_Cannot_be_greater_than_EndDate</fullName>
        <active>true</active>
        <description>R6 - Created as part of WorkId - 349 - Start Date cannot be bigger than the end Date</description>
        <errorConditionFormula>AND(NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),Start_Date__c &gt;  End_Date__c)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Start date should be less than or equal to End date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Visit_Report_Status_Closed_Check</fullName>
        <active>true</active>
        <description>R5 - If Account Plan Status is Closed, then the user should not edit the Visit Report.</description>
        <errorConditionFormula>AND(NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),ISPICKVAL(Account_Plan__r.Status__c, &apos;Closed&apos;))</errorConditionFormula>
        <errorMessage>You cannot save the changes - Account Plan is Closed</errorMessage>
    </validationRules>
