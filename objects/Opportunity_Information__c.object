if(&quot;{!Apttus__APTS_Agreement__c.Apttus__Status_Category__c}&quot; ==&apos;Request&apos;||&quot;{!Apttus__APTS_Agreement__c.Apttus__Status_Category__c}&quot; ==&apos;In Authoring&apos;||&quot;{!Apttus__APTS_Agreement__c.Apttus__Status_Category__c}&quot; ==&apos;In Signatures&apos;) 
{ 
alert( &quot;You cannot create an Opportunity from an in-flight Agreement&quot; ); 
} 
else 
{ 
var Accagreement = sforce.connection.query(&quot;SELECT id,Apttus__Account__c ,Apttus__Account__r.Channel_Class_Level1__c,Apttus__Account__r.Channel_Class_Level2__c from Apttus__APTS_Agreement__c where id =&apos;{!Apttus__APTS_Agreement__c.Id }&apos; limit 1&quot;); 

var records = Accagreement.getArray(&quot;records&quot;); 
var agreementInfo = sforce.connection.query(&quot;SELECT id,Name FROM APTS_MSPA_Information__c WHERE Agreement__c =&apos;{!Apttus__APTS_Agreement__c.Id }&apos; limit 1&quot;); 
var inforecord = agreementInfo.getArray(&quot;records&quot;); 

var url1 = &apos;/006/e?opp4_lkid={!Apttus__APTS_Agreement__c.Apttus__AccountId__c}&amp;opp4={!Apttus__APTS_Agreement__c.Apttus__Account__c}&amp;CF{!$Label.CLM_ParentAgrmntInOpty}=&apos;+inforecord[0].Name+&apos;&amp;CF{!$Label.CLM_ParentAgrmntInOpty}_lkid={!Apttus__APTS_Agreement__c.Agreement_Information_ID__c}&amp;opp11=Prospecting&amp;00N20000009cSjs=Captured&amp;00N20000009vIbw={!JSENCODE( TEXT(User.Application_BU__c))}&amp;00N20000009vIbs={!JSENCODE(TEXT(User.Business_Line_BU__c))}&amp;00N20000009vIbx=&apos;+records[0].Apttus__Account__r.Channel_Class_Level1__c+&apos;&amp;00N20000009vIby=&apos;+records[0].Apttus__Account__r.Channel_Class_Level2__c+&apos;&amp;retURL={!Apttus__APTS_Agreement__c.Id}&apos; 
parent.window.location.replace(url1); 
}</url>
