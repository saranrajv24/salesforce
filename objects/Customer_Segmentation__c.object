    <validationRules>
        <fullName>BU_is_Mandatory</fullName>
        <active>true</active>
        <description>R7B â€“ Account Segmentation Enhancements</description>
        <errorConditionFormula>AND(
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), 
 ISPICKVAL(Segment_Level__c,&apos;Business Unit&apos;), ISBLANK(TEXT(Business_Line_BU__c))
)</errorConditionFormula>
        <errorMessage>You have chosen to Segment by Business Unit. You must fill in a BU in this case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Industry_and_Channel_Class</fullName>
        <active>true</active>
        <description>R7B</description>
        <errorConditionFormula>AND(
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
SyncwithAccount__c,
RecordType.DeveloperName == &quot;Customer_Sales_Segmentation&quot;,
NOT(Bypass_Sync_Validation__c),
OR(ISCHANGED(ABB_Industry_Usage_Level1__c),ISCHANGED(ABB_Industry_Usage_Level2__c),ISCHANGED(ABB_Industry_Usage_Level3__c),
ISCHANGED(Channel_Class_Level1__c),ISCHANGED(Channel_Class_Level2__c))
)</errorConditionFormula>
        <errorMessage>You cannot change the field when sync is enabled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Division_is_Mandatory</fullName>
        <active>true</active>
        <description>R7B â€“ Account Segmentation Enhancements</description>
        <errorConditionFormula>AND(
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
ISBLANK(TEXT(DIV__c)),
OR( ISPICKVAL(Segment_Level__c, &apos;Division&apos;),
ISPICKVAL(Segment_Level__c, &apos;Business Unit&apos;), 
ISPICKVAL(Segment_Level__c, &apos;Product Line&apos;), 
ISPICKVAL(Segment_Level__c, &apos;Product Group&apos;), 
ISPICKVAL(Segment_Level__c, &apos;Division Service&apos;)))</errorConditionFormula>
        <errorDisplayField>DIV__c</errorDisplayField>
        <errorMessage>Division is Mandatory for the selected Segment level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Account_Segmentation_On_Bulk_Account</fullName>
        <active>true</active>
        <description>Related Account cannot be Bulk Business.</description>
        <errorConditionFormula>AND(
    NOT($User.AccountMerge_Override__c),
    NOT($Setup.Validation_Override__c.Exclude__c),
    CONTAINS(LOWER(Account__r.Name), &apos;bulk business&apos;),
    NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
    NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c) 
)</errorConditionFormula>
        <errorMessage>Related Account cannot be Bulk Business.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Segment_Level_Validation</fullName>
        <active>true</active>
        <description>R5:Segment level must always match the hierarchy entered by the user in Division/Business Unit/Product Group/Product Line</description>
        <errorConditionFormula>AND(
NOT($Setup.Validation_Override__c.Exclude__c),
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
OR( 
(TEXT(Business_Line_BU__c) != NULL &amp;&amp; TEXT(Segment_Level__c) == &apos;Division&apos;),
( (Product_Group__c != NULL || Product__c != NULL) &amp;&amp; (TEXT(Segment_Level__c) == &apos;Division&apos; || TEXT(Segment_Level__c) == &apos;Business Unit&apos; || TEXT(Segment_Level__c) == &apos;ABB&apos; || TEXT(Segment_Level__c) == &apos;ABB Service&apos;)),
( (Product__c != NULL) &amp;&amp; (TEXT(Segment_Level__c) != &apos;Product Line&apos;)),
( (TEXT(Business_Line_BU__c) != NULL || Product_Group__c != NULL || Product__c != NULL) &amp;&amp; (TEXT(Segment_Level__c) == &apos;Division Service&apos;))
))</errorConditionFormula>
        <errorMessage>Segment level must always match the hierarchy entered by the user in Division/Business Unit/Product Group/Product Line</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Share_Of_Wallet_Range</fullName>
        <active>true</active>
        <description>R7B - Account Segmentation Enhancements</description>
        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
OR(Share_of_Wallet__c &lt; 0,Share_of_Wallet__c &gt; 100))</errorConditionFormula>
        <errorMessage>Value cannot be below 0 or 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sync_cannot_be_false_if_DIV_is_PG</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), 
Text(DIV__c) == &apos;PG&apos;,
SyncwithAccount__c &lt;&gt; TRUE
)</errorConditionFormula>
        <errorMessage>Sync should always be enabled when Division is PG</errorMessage>
    </validationRules>
