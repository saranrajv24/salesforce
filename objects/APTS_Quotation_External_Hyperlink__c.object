    <validationRules>
        <fullName>APTS_AccessLevel_Validation</fullName>
        <active>true</active>
        <description>User should not be able to change Access level if user is not the owner of that QEH record.</description>
        <errorConditionFormula>IF( AND(ISCHANGED(APTS_Access_level__c),$User.Id  != OwnerId) , true, false)</errorConditionFormula>
        <errorMessage>This operation is not allowed. Only the owner of the document can change the access level.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APTS_URL_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(BEGINS(APTS_URL__c, &apos;http://&apos;), BEGINS(APTS_URL__c, &apos;https://&apos;)))</errorConditionFormula>
        <errorDisplayField>APTS_URL__c</errorDisplayField>
        <errorMessage>Please check the URL entered and ensure it is entered in the correct format.

Correct formats include https:// OR http:// at the beginning or the URL</errorMessage>
    </validationRules>
