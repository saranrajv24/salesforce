    <fields>
        <fullName>APTS_Estimated_Sales_Price__c</fullName>
        <externalId>false</externalId>
        <formula>IF( AND(  Quote_Proposal__r.APTS_Quotation_Amount__c &gt; 0,NOT(ISBLANK(  Quote_Proposal__r.Apttus_Proposal__Opportunity__c  ) ) ) , 

( Quote_Proposal__r.APTS_Quotation_Amount__c + ((VALUE(TEXT(APTS_Percentage__c)) / 100) * Quote_Proposal__r.APTS_Quotation_Amount__c)), 

(Quote_Proposal__r.Apttus_Proposal__Opportunity__r.Opportunity_Value__c + ((VALUE(TEXT(APTS_Percentage__c)) / 100) * Quote_Proposal__r.Apttus_Proposal__Opportunity__r.Opportunity_Value__c)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Estimated Sales Price calculated with the input in the Percentage field above.</inlineHelpText>
        <label>Estimated Sales Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
