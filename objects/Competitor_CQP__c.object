    <validationRules>
        <fullName>APTS_CannotChangeCompetitorAccount</fullName>
        <active>false</active>
        <description>Competitor account cannot be changed after creation of it.
Added as a part of CCP.</description>
        <errorConditionFormula>AND( OR(Quote_Proposal__r.APTS_Record_Type__c = &apos;Transactional Quotation&apos;, Quote_Proposal__r.APTS_Record_Type__c = &apos;Frame Agreement&apos;) ,ISCHANGED(Account__c))</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>You are not allowed to change the Competitor Account. Please add a new Competitor.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APTS_Enable_Winning_Company_upon_Closure</fullName>
        <active>true</active>
        <errorConditionFormula>AND( APTS_Winning_Company__c  = TRUE, 
NOT($User.AccountMerge_Override__c),
NOT($User.Bypass_Opportunity_Validations__c),
NOT($Setup.Validation_Override__c.Exclude__c),

OR ( 
ISPICKVAL(Quote_Proposal__r.Apttus_Proposal__Approval_Stage__c  ,&quot;RFQ Received&quot;), 
ISPICKVAL(Quote_Proposal__r.Apttus_Proposal__Approval_Stage__c, &quot;RFQ Assigned&quot;), 
ISPICKVAL(Quote_Proposal__r.Apttus_Proposal__Approval_Stage__c, &quot;Draft&quot;), 
ISPICKVAL(Quote_Proposal__r.Apttus_Proposal__Approval_Stage__c, &quot;Approval Required&quot;), 
ISPICKVAL(Quote_Proposal__r.Apttus_Proposal__Approval_Stage__c, &quot;Pending Approval&quot;), 
ISPICKVAL(Quote_Proposal__r.Apttus_Proposal__Approval_Stage__c, &quot;Approved&quot;), 
ISPICKVAL(Quote_Proposal__r.Apttus_Proposal__Approval_Stage__c, &quot;Rejected&quot;), 
ISPICKVAL(Quote_Proposal__r.Apttus_Proposal__Approval_Stage__c, &quot;Generated&quot;), 
ISPICKVAL(Quote_Proposal__r.Apttus_Proposal__Approval_Stage__c, &quot;Under Support&quot;)

) 
)</errorConditionFormula>
        <errorDisplayField>APTS_Winning_Company__c</errorDisplayField>
        <errorMessage>The Winning Company can be selected only upon submission to customer or upon quotation closure.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APTS_Only_1_Winning_Company_Allowed</fullName>
        <active>true</active>
        <description>Validation rule to allow only 1 Winning Company to be selected per Quotation and can not be selected if the Quotation is Won.</description>
        <errorConditionFormula>AND(
NOT($User.AccountMerge_Override__c),
NOT($User.Bypass_Opportunity_Validations__c),
NOT($Setup.Validation_Override__c.Exclude__c),
APTS_Winning_Company__c = TRUE,
 
OR( Quote_Proposal__r.APTS_Record_Type__c = &quot;Transactional Quotation&quot;, 
Quote_Proposal__r.APTS_Record_Type__c= &quot;Frame Agreement&quot; 

),
OR( ISPICKVAL(Quote_Proposal__r.Apttus_Proposal__Approval_Stage__c, &quot;Won&quot;),
Quote_Proposal__r.APTS_Count_Winner_Competitors__c &gt;= 1
)
)</errorConditionFormula>
        <errorDisplayField>APTS_Winning_Company__c</errorDisplayField>
        <errorMessage>Only 1 Winning Company can be selected per Quotation and the Winning company cannot be selected when the Quotation is Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Competitor_No_Bulk_Accounts</fullName>
        <active>true</active>
        <description>Account cannot be a Bulk Account.</description>
        <errorConditionFormula>AND(
    NOT($User.AccountMerge_Override__c),
    NOT($Setup.Validation_Override__c.Exclude__c), 
    CONTAINS(LOWER(Account__r.Name), &apos;bulk business&apos;)    
)</errorConditionFormula>
        <errorMessage>Account cannot be a Bulk Account.</errorMessage>
    </validationRules>
