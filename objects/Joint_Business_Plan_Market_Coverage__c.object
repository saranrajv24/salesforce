        <errorConditionFormula>AND(NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),OR(ISPICKVAL( Joint_Business_Plan__r.Status__c , &apos;Closed&apos;), 
