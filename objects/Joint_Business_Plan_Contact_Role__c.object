    <webLinks>
        <fullName>Initiate_NPS_survey</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Initiate NPS survey</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/apex.js&quot;)}
var allowWarning = true;
var usrId = &apos;{!$User.Id}&apos;; 
if (&apos;{!Joint_Business_Plan__c.NPS_ID__c}&apos; != &apos;&apos;) {
    alert(&apos;NPS Survey created. There can be only one NPS survey per business plan&apos;);
} else {
	var selectedIds = {!GETRECORDIDS($ObjectType.Joint_Business_Plan_Contact_Role__c)};
	if(&apos;{!$User.ProfileId}&apos; == &apos;{!$Label.CL00075}&apos; || usrId == &apos;{!Joint_Business_Plan__c.OwnerId}&apos;){ 
		if (selectedIds[0] == null) {
			alert(&quot;You must select at least one record&quot;);
		} else if (&apos;{!Joint_Business_Plan__c.Status__c}&apos; == &apos;Obsolete&apos; || &apos;{!Joint_Business_Plan__c.Status__c}&apos; == &apos;Draft&apos;) {	
			alert(&apos;The Survey should be available if the Business Plan is not in Status &quot;Obsolete&quot; or &quot;Draft&quot;&apos;);
		} else {
			window.location = &apos;/apex/InitiateNPSSurvey?BusinessPlanId=&apos;+&apos;{!Joint_Business_Plan__c.Id}&amp;ContactRoleIds=&apos;+selectedIds.toString();		
		}
	} else {
		alert(&quot;Only System admin or Business Plan Owner can initiate the Survey&quot;);
	}
}</url>
    </webLinks>
    <webLinks>
        <fullName>Mass_Copy</fullName>
        <availability>online</availability>
        <description>R5 - Custom Button which copies all Contacts linked to the particular Account without duplicates.</description>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Copy all from Account</masterLabel>
        <openType>noSidebar</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>/apex/JBPContactRoleCopy?Id={!Joint_Business_Plan__c.Id}</url>
    </webLinks>
    <webLinks>
        <fullName>New_Contact_Role</fullName>
        <availability>online</availability>
        <description>R6: Custombutton to redirect to VF page to choose the contacts related to the Account</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Select from Account</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)};
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/apex.js&quot;)};

var access = &apos;Edit&apos;;
var usrId = &apos;{!$User.Id}&apos;;

var queriedRec = sforce.connection.query(&quot;Select Id,AccessLevel, UserOrGroupId from Joint_Business_Plan__Share where AccessLevel=&apos;&quot;+access+&quot;&apos; AND ParentId=&apos;&quot;+&apos;{!Joint_Business_Plan__c.Id}&apos;+&quot;&apos; AND UserorGroupId=&apos;&quot;+usrId+&quot;&apos; LIMIT 1&quot;);

var records = queriedRec.getArray(&quot;records&quot;);
if(&apos;{!$User.ProfileId}&apos; == &apos;{!$Label.CL00075}&apos; || usrId == &apos;{!Joint_Business_Plan__c.OwnerId}&apos;){
window.location=&apos;/apex/ChannelPartnerContactRolesVF?Id=&apos;+&apos;{!Joint_Business_Plan__c.Id}&apos;+&apos;&amp;AccId=&apos;+&apos;{!Joint_Business_Plan__c.AccountId__c}&apos;;
}
else if(&apos;{!$User.ProfileId}&apos; != &apos;{!$Label.CL00075}&apos; &amp;&amp; records.length &gt; 0){
window.location=&apos;/apex/ChannelPartnerContactRolesVF?Id=&apos;+&apos;{!Joint_Business_Plan__c.Id}&apos;+&apos;&amp;AccId=&apos;+&apos;{!Joint_Business_Plan__c.AccountId__c}&apos;;
}
else if(&apos;{!$User.ProfileId}&apos; != &apos;{!$Label.CL00075}&apos; &amp;&amp; records.length &lt; 1){
alert(&apos;Please request Write access as part of your business plan team&apos;);
}</url>
    </webLinks>
