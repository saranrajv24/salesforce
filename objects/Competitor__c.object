    <validationRules>
        <fullName>Competitor_Channel_Class_check</fullName>
        <active>false</active>
        <description>Craeted as part of R5-2347.</description>
        <errorConditionFormula>OR(  ISPICKVAL(Competitor__r.Channel_Class__c, &apos;ABBINT&apos;), ISPICKVAL(Competitor__r.Channel_Class__c, &apos;DEU&apos;))</errorConditionFormula>
        <errorMessage>ABB Internal accounts and Direct End User accounts for the opportunity cannot be added as competitors.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Competitor_No_Bulk_Accounts</fullName>
        <active>true</active>
        <description>Account End User from related Opportunity and Competitor Account cannot be Bulk Accounts.</description>
        <errorConditionFormula>AND(
    NOT($User.AccountMerge_Override__c),
    NOT($Setup.Validation_Override__c.Exclude__c),    
    OR(
        CONTAINS(LOWER(Opportunity__r.Account.Name), &apos;bulk business&apos;),        
        CONTAINS(LOWER(Competitor__r.Name), &apos;bulk business&apos;)
    )
)</errorConditionFormula>
        <errorMessage>Account End User from related Opportunity and Competitor Account cannot be Bulk Accounts.</errorMessage>
    </validationRules>
