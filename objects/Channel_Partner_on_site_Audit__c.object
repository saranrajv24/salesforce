    <validationRules>
        <fullName>Validate_the_fields_on_status_change</fullName>
        <active>true</active>
        <description>R7 - Validate the required fields are filled out to change status to Completed</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &apos;Completed&apos;), 
OR( 
AND( 
RecordType.Name = &quot;Sales&quot;, OR(ISPICKVAL(Status__c,&apos;&apos;), ISNULL( Date_of_onsite_Audit__c ), ISNULL(Number_of_Sales_People_trained__c), NOT(Warranty_handling_procedure_known__c), 
NOT(Process_for_fast_order_handling_known__c), NOT(Sales_support_available_from_ABB_known__c), NOT(Access_and_user_knowledge_to_sales_tools__c), NOT(Product_brochures_and_flyers_available__c)) 
),	
AND ( 
RecordType.Name = &quot;Service&quot;, OR(ISNULL(No_of_people_with_service_authorization__c), NOT(Report_for_field_repair_available__c), ISPICKVAL(Access_to_service_spares__c, &apos;&apos;), 
NOT(Tools_collaboration_system_is_in_use__c), NOT(Relevant_HSE_aspects_know_and_followed__c), NOT(Spare_part_operations_handled_by_partner__c), NOT(Competent_people_for_spare_parts__c), NOT(Warehouse_suitable_for_stocking__c), NOT(Warranty_repairs_handled_by_partner__c), NOT(Competent_field_service_personnel__c), NOT(ABB_Warranty_rules_and_instructions__c), 
NOT(Access_to_all_warranty_material__c), NOT(Defective_material_stored_and_handled__c)) 
),	
AND ( 
RecordType.Name = &quot;Support&quot;, OR(ISPICKVAL(Status__c,&apos;&apos;), ISNULL( Date_of_onsite_Audit__c ), NOT(Customer_Access__c), NOT(Technical_support_available_from_ABB__c),ISPICKVAL(Commissioning_provided_actively__c,&apos;&apos;),ISNULL(personnel_for_commissioning__c),
ISNULL(of_persons_technically_authorized__c)) 
), 
AND ( 
RecordType.Name = &quot;Engineering&quot;, OR(ISNULL(persons_for_engineering__c), NOT(ESD_handling_process_known_and_utilized__c), ISPICKVAL(Own_Cabinet_panel_building__c, &apos;&apos;)) 
)	
))</errorConditionFormula>
        <errorMessage>Status cannot be changed as not all Red Flag marked mandatory fields are answered. You might need to change Status back to &quot;Draft&quot; to save and see if Red Flagged fields were missed in earlier stages</errorMessage>
    </validationRules>
