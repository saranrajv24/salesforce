    <validationRules>
        <fullName>Check_Account_Plan_Year</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), 
ISPICKVAL(Status__c, &apos;Closed&apos;), ISCHANGED(Account_Plan_Year__c ))</errorConditionFormula>
        <errorMessage>You can not edit Account Plan Year when Account Plan is closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Account_Plans_On_Bulk_Accounts</fullName>
        <active>true</active>
        <description>Related Account cannot be Bulk Business.</description>
        <errorConditionFormula>AND(
    NOT($User.AccountMerge_Override__c),
    NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
    NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), 
    CONTAINS(LOWER(Account__r.Name), &apos;bulk business&apos;)
)</errorConditionFormula>
        <errorMessage>Related Account cannot be Bulk Business.</errorMessage>
    </validationRules>
