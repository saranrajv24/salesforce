        <externalId>false</externalId>
        <formula>IF( AND( APTS_Quotation_Line_Item__r.APTS_Total_Net_Sales_Price__c  &gt; 0,NOT(ISBLANK( APTS_Quotation_Line_Item__r.Apttus_Proposal__Proposal__r.Apttus_Proposal__Opportunity__c ) ) )  , 

( APTS_Quotation_Line_Item__r.APTS_Total_Net_Sales_Price__c   + ((VALUE(TEXT(APTS_Percentage__c)) / 100) * APTS_Quotation_Line_Item__r.APTS_Total_Net_Sales_Price__c)), 

(APTS_Quotation_Line_Item__r.Apttus_Proposal__Proposal__r.Apttus_Proposal__Opportunity__r.Opportunity_Value__c  + ((VALUE(TEXT(APTS_Percentage__c)) / 100) * APTS_Quotation_Line_Item__r.Apttus_Proposal__Proposal__r.Apttus_Proposal__Opportunity__r.Opportunity_Value__c)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Estimated Sales Price calculated with the input in the Percentage field above.</inlineHelpText>
        <label>Estimated Sales Price</label>
        <precision>18</precision>
        <required>false</required>
