        <label>XML Tag</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>sObject__c</fullName>
        <description>the sObject API Name to where the value from the xml will be mapped to</description>
        <externalId>false</externalId>
