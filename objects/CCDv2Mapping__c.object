<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This custom setting will provide a mapping for xml tags to object fields. This setting is consumed by the class ClsCCDMapping</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Field_Name__c</fullName>
        <description>Provide a field API Name to where to map the xml value to</description>
        <externalId>false</externalId>
        <inlineHelpText>Provide a field API Name to where to map the xml value to</inlineHelpText>
        <label>Field Name</label>
        <length>200</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Type__c</fullName>
        <externalId>false</externalId>
        <label>Type</label>
        <length>200</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>XML_Tag__c</fullName>
        <description>This field sets the xml tag name</description>
        <externalId>false</externalId>
