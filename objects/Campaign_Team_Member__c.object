    <webLinks>
        <fullName>Add_Team_Member</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add Team Member</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)}

result = sforce.connection.query(&quot;Select Id,Team_Member__c,Access_Level__c,Role__c from Campaign_Team_Member__c where Access_Level__c = &apos;Read/Write&apos; AND Campaign__c=&apos;{!Campaign.Id}&apos;&quot;);

records = result.getArray(&quot;records&quot;);
var flag = false;
var retURL = &apos;/apex/AddCampaignTeamMember?id={!Campaign.Id}&apos;

for (var i=0; i&lt; records.length; i++) {
	var record = records[i];
	var teamUser = record.Team_Member__c;
	var teamUserId = teamUser.substring(0,15);

	if(teamUserId == &apos;{!$User.Id}&apos;){
	  flag = true;
	}
}

if(&apos;{!$User.Id}&apos; == &apos;{!JSENCODE(Campaign.OwnerId)}&apos; || &apos;{!$Profile.Name}&apos; == &apos;System Administrator&apos; || flag == true){
	window.open(retURL,&apos;_top&apos;);
}
else{
	alert(&apos;You do not have permission to add team member&apos;);
}</url>
