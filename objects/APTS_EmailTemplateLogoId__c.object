<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>This custom setting will hold the Id of ABB Logo(which is being used in email templates), which is stored in Documents.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>APTS_Document_Id__c</fullName>
        <description>Copy the Document Image(company Logo - that you need to display on MSPA &amp; Quotation Email Templates ) 15-digit Id from URL to this field</description>
        <externalId>false</externalId>
        <label>Document Id</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Email Template Logo Id</label>
    <visibility>Public</visibility>
</CustomObject>
