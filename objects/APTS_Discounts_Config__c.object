    </validationRules>
    <validationRules>
        <fullName>APTS_Missing_Hierarchy_Level</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(APTS_Hierarchy_Level_MS__c)</errorConditionFormula>
        <errorDisplayField>APTS_Hierarchy_Level_MS__c</errorDisplayField>
        <errorMessage>Please select value for hierarchy level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APTS_Missing_Product_Discounts</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(APTS_Net_Discount__c),
NOT(APTS_Additional_Discount__c),
NOT(APTS_Substitute_Discount__c)
)</errorConditionFormula>
        <errorMessage>Please check at least one value from product discounts section</errorMessage>
    </validationRules>
