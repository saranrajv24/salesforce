    <webLinks>
        <fullName>New_Results</fullName>
        <availability>online</availability>
        <description>R5 - Created as part of req 2461</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Results</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)}


var result1 = sforce.connection.query(&quot;Select Id,Name from Rfp_Results__c where Rfp_Results__c = &apos;{!RFP__c.Id}&apos; &quot;); 
var records1 = result1.getArray(&quot;records&quot;); 
var flag1 = false; 

if(records1.length&gt;0){ 
flag1 = true; 
alert(&apos;Only one Result record per RFQ is allowed.&apos;); 
} 
result = sforce.connection.query(&quot;Select Id,Name,Team_Role__c,User__c,Access_Level__c from RFP_Team__c where Access_Level__c = &apos;Read/Write&apos; &quot;);
records = result.getArray(&quot;records&quot;);
var flag = false;


var redurl = &apos;{!$Label.System_Results}e?CF{!$Label.System_Result}={!URLENCODE(RFP__c.Name)}&amp;CF{!$Label.System_Result}_lkid={!URLENCODE(RFP__c.Id)}&amp;retURL=/{!URLENCODE(RFP__c.Id)}&apos;

for (var i=0; i&lt; records.length; i++){
  var record = records[i];
  var teamUser = record.User__c;
  var teamUserId = teamUser.substring(0,15);

  if(teamUserId == &apos;{!$User.Id}&apos;){
  flag = true;
}
}   

if(&apos;{!$User.Id}&apos; == &apos;{!JSENCODE(RFP__c.OwnerId)}&apos; || &apos;{!$Profile.Name}&apos; == &apos;System Administrator&apos; || &apos;{!$User.Id}&apos; == &apos;{!JSENCODE(RFP__c.Rfp_Account_Manager__c)}&apos; || flag == true){
if(flag1 == false) 
{
  window.open(redurl,&apos;_top&apos;);
}
}
else{
  alert(&apos;Insufficient Privileges to create Result records.&apos;);
}</url>
    </webLinks>
