    <validationRules>
        <fullName>One_Result_Record_per_RFQ</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW()  &amp;&amp;  Rfp_Results__r.Total_Results_Associated__c   =  1</errorConditionFormula>
        <errorMessage>There exists a Result Record for this RFQ. Each RFQ record can have only one Result record.</errorMessage>
