    <validationRules>
        <fullName>Account_GUID_or_ICVID_is_Mandatory</fullName>
        <active>true</active>
        <description>This validation rule will ensure that either Guid or ICV-id exists, but not both</description>
        <errorConditionFormula>AND(
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
IF(OR(AND( ISBLANK(Account_GUID__c) , ISBLANK(Account_ICVID__c)),AND( NOT(ISBLANK(Account_GUID__c)) , NOT(ISBLANK(Account_ICVID__c)))),true,false)
)</errorConditionFormula>
        <errorMessage>Either Account GUID or Account ICVID must be provided</errorMessage>
    </validationRules>
