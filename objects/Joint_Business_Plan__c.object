    <validationRules>
        <fullName>Date_for_Next_Review_should_not_be_past</fullName>
        <active>true</active>
        <description>R6: . When save a business plan if the Date for Next Review is in the past an error should be display</description>
        <errorConditionFormula>(ISCHANGED( Status__c) || ISCHANGED(  Assigned_To__c ) || ISCHANGED(Date_for_Next_Review__c)) &amp;&amp; Date_for_Next_Review__c &lt; TODAY() &amp;&amp; NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c) &amp;&amp;
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c)</errorConditionFormula>
        <errorMessage>You can not change the Status / Assigned To/ Date for Next Review field when the  Date for Next Review is in past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_greater_than_Start_Date</fullName>
        <active>true</active>
        <description>R5 - Joint Business Plan End Date must be always greater than Start Date.</description>
        <errorConditionFormula>AND(NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), 
End_Date__c  &lt;  Start_Date__c )</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End Date must be greater than Start Date!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>JBP_status_change_to_In_Approval</fullName>
        <active>true</active>
        <description>R5 - Validate the required fields are filled out to change status to &quot;In Approval&quot;  or &quot;Closed&quot;</description>
        <errorConditionFormula>AND(NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c) , 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),OR( 
AND(OR(RecordType.DeveloperName = &quot;Distributor_as_AVP&quot;,RecordType.DeveloperName =&quot;Technical_Channel_as_Key_Strategic_Partner_or_AVP&quot;),OR(ISPICKVAL(Status__c, &quot;In Approval&quot;),ISPICKVAL(Status__c, &quot;Closed&quot;), ISPICKVAL(Status__c, &quot;Active&quot;)), 
OR(  ISBLANK(Existing_ABB_offering__c), ISBLANK(Suppliers_with_potential_ABB_offering__c ), ISBLANK(Suppliers_with_complementary_products__c), ISBLANK(Co_operation_Overview_1__c), ISBLANK(Co_operation_Overview_2__c),  LEN(Training_and_Authorization_Plan__c) &lt; 1, Channel_Market_Coverage__c = NULL, Actions_RollUp__c = 0,JBP_ProdLine_Rollup__c = 0, JBP_MarketCoverage_Rollup__c = 0, JBP_Contact_Role_Rollup__c = 0, ISBLANK( Date_for_Next_Review__c ),ISBLANK( Assigned_To__c ) ) ), 

AND(RecordType.DeveloperName = &quot;Distributor_as_Strategic_Key_Partner&quot;, OR(ISPICKVAL(Status__c, &quot;In Approval&quot;),ISPICKVAL(Status__c, &quot;Closed&quot;), ISPICKVAL(Status__c, &quot;Active&quot;)), 
OR(ISBLANK(Existing_ABB_offering__c), ISBLANK(Suppliers_with_potential_ABB_offering__c ), ISBLANK( Suppliers_with_complementary_products__c), ISBLANK(Co_operation_Overview_1__c), ISBLANK(Co_operation_Overview_2__c), Channel_Market_Coverage__c = NULL, Actions_RollUp__c = 0,JBP_ProdLine_Rollup__c = 0, JBP_MarketCoverage_Rollup__c = 0, JBP_Contact_Role_Rollup__c = 0, ISBLANK(Date_for_Next_Review__c),ISBLANK( Assigned_To__c ))), 

AND(RecordType.DeveloperName =&quot;Distributor_as_Partner&quot;, OR(ISPICKVAL(Status__c, &quot;In Approval&quot;),ISPICKVAL(Status__c, &quot;Closed&quot;), ISPICKVAL(Status__c, &quot;Active&quot;)),  OR( ISBLANK(Existing_ABB_offering__c), ISBLANK(Suppliers_with_potential_ABB_offering__c ), ISBLANK( Suppliers_with_complementary_products__c), ISBLANK(Co_operation_Overview_1__c), ISBLANK(Co_operation_Overview_2__c), Channel_Market_Coverage__c = NULL, Actions_RollUp__c = 0,JBP_ProdLine_Rollup__c = 0,JBP_Contact_Role_Rollup__c = 0 , ISBLANK( Date_for_Next_Review__c ),ISBLANK( Assigned_To__c )) ), 

AND(RecordType.DeveloperName =&quot;Technical_Channel_as_Partner&quot;, OR(ISPICKVAL(Status__c, &quot;In Approval&quot;),ISPICKVAL(Status__c, &quot;Closed&quot;), ISPICKVAL(Status__c, &quot;Active&quot;)),
OR(ISBLANK(Existing_ABB_offering__c), ISBLANK(Suppliers_with_potential_ABB_offering__c ), ISBLANK( Suppliers_with_complementary_products__c), ISBLANK(Co_operation_Overview_1__c), ISBLANK(Co_operation_Overview_2__c), Channel_Market_Coverage__c = NULL, Actions_RollUp__c = 0, JBP_Contact_Role_Rollup__c = 0 , JBP_MarketCoverage_Rollup__c = 0, ISBLANK( Date_for_Next_Review__c ),ISBLANK( Assigned_To__c ))) 
)
)</errorConditionFormula>
        <errorMessage>Status cannot be changed as not all Red Flag marked mandatory fields are answered. You might need to change Status back to &quot;Draft&quot; to save and see if Red Flagged fields were missed in earlier stages or related lists.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Joint_Business_Plan_On_Bulk_Accounts</fullName>
        <active>true</active>
        <description>Related Account cannot be Bulk Business.</description>
        <errorConditionFormula>AND(
    NOT($User.AccountMerge_Override__c),
 NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c),NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c),
    CONTAINS(LOWER(Account__r.Name), &apos;bulk business&apos;)
)</errorConditionFormula>
        <errorMessage>Related Account cannot be Bulk Business.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Clone_Business_Plan</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Transfer to next Year</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/apex.js&quot;)}

var btnService = document.getElementsByName(&quot;clone_business_plan&quot;);
//btnService[0].value=&apos;Please wait...&apos;; //Change the text
btnService[0].disabled=true;

var result = sforce.apex.execute(&quot;copyChPartnerBusPlan&quot;,&quot;copyBusPlan&quot;, {chPartnrBusPlanId:&apos;{!Joint_Business_Plan__c.Id}&apos;,
dupRecrdId:&apos;{!Joint_Business_Plan__c.Business_Plan_Transferred_To__c}&apos;,  
accountId:&apos;{!Joint_Business_Plan__c.AccountId__c}&apos;,
status:&apos;{!Joint_Business_Plan__c.Status__c}&apos;});


if (result.toString().includes(&quot;Success&quot;)) {
        var arr = result.toString().split(&quot;:&quot;); 
	alert(&apos;Business Plan is cloned successfully. &apos; + arr[1]);
	var arr = result.toString().split(&quot;:&quot;);       
	window.location = &apos;/&apos;+ arr[2];
} else {
	alert(result);
        btnService[0].disabled=false;
}</url>
    </webLinks>
