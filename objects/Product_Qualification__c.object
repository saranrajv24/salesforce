    <validationRules>
        <fullName>Minus_Values_Check</fullName>
        <active>true</active>
        <description>R4 Cannot enter Minus value in Available_Market_CAPEX__c, Available_Market_OPEX__c,Actual_Cost_USD__c, Estimated_Cost_USD__c</description>
        <errorConditionFormula>AND(
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), 
NOT($User.AccountMerge_Override__c),OR(OR(Available_Market_CAPEX__c &lt; 0 , Available_Market_OPEX__c &lt; 0), AND(NOT(ISNULL(Estimated_Cost_USD__c)), Estimated_Cost_USD__c &lt; 0),
AND(NOT(ISNULL(Actual_Cost_USD__c)), Actual_Cost_USD__c &lt; 0)))</errorConditionFormula>
        <errorMessage>You cannot enter minus values</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Product_Qualification_On_Bulk_Account</fullName>
        <active>true</active>
        <description>Related Account cannot be Bulk Business.</description>
        <errorConditionFormula>AND(
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), 
    NOT($User.AccountMerge_Override__c),
    NOT($Setup.Validation_Override__c.Exclude__c),
    CONTAINS(LOWER(Account__r.Name), &apos;bulk business&apos;)
)</errorConditionFormula>
        <errorMessage>Related Account cannot be Bulk Business.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Target_End_Date_cannot_be_Past_Date</fullName>
        <active>true</active>
        <description>R4 Check the Target End Date cannot be entered as a past date</description>
        <errorConditionFormula>AND(
NOT($Setup.ObjectsSaveMode__c.DisabledValidation__c), 
NOT($Setup.ObjectsSaveMode__c.FullLogicDisabled__c), 
NOT($User.AccountMerge_Override__c),NOT($Setup.Validation_Override__c.Account_Merge_Override__c) ,Target_End_Date__c &lt; TODAY())</errorConditionFormula>
        <errorDisplayField>Target_End_Date__c</errorDisplayField>
        <errorMessage>The Target End Date cannot be entered as a past date</errorMessage>
    </validationRules>
