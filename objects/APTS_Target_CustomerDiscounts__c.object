        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Product__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Product Hierarchy</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>1 AND 2 AND 3</booleanFilter>
            <errorMessage>Record does not exist or is not a hierarchy</errorMessage>
            <filterItems>
                <field>Product2.APTS_LP_Is_Hierarchy__c</field>
                <operation>equals</operation>
                <value>True</value>
            </filterItems>
            <filterItems>
                <field>$User.Default_LSO__c</field>
                <operation>contains</operation>
                <valueField>Product2.APTS_LP_Sales_Organization__c</valueField>
            </filterItems>
            <filterItems>
                <field>Product2.APTS_Hierarchy_Level__c</field>
                <operation>equals</operation>
                <value>4</value>
            </filterItems>
            <infoMessage>Search results limited to hierarchies</infoMessage>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Product2</referenceTo>
        <relationshipLabel>Target Discount Hierarchies</relationshipLabel>
        <relationshipName>Target_Customer_Info</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Recovery2__c</fullName>
        <externalId>false</externalId>
        <label>Additional discount for Target Customer</label>
        <precision>5</precision>
        <required>true</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Validity_Date__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>can be left empty or filled with a date in the future. entries with dates in the past will be excluded from calculations</inlineHelpText>
