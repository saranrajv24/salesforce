    <validationRules>
        <fullName>Hierarchy_set</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Product__c)</errorConditionFormula>
        <errorDisplayField>Product__c</errorDisplayField>
        <errorMessage>Hierarchy must be selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Exp_Target_Discount</fullName>
        <active>true</active>
        <errorConditionFormula>Exp_Target_Discount__c &lt; 0 || Exp_Target_Discount__c &gt; 1</errorConditionFormula>
        <errorDisplayField>Exp_Target_Discount__c</errorDisplayField>
        <errorMessage>Expected Target Discount must be between 0 and 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Recovery</fullName>
        <active>true</active>
        <errorConditionFormula>Recovery2__c &lt; 0 || Recovery2__c &gt; 1</errorConditionFormula>
        <errorDisplayField>Recovery2__c</errorDisplayField>
        <errorMessage>Recovery2 must be between 0 and 100</errorMessage>
    </validationRules>
